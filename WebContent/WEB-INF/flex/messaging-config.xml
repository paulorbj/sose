		<adapter-definition id="FaturamentoMessageService"
			class="br.com.sose.mensagens.FaturamentoMessageService" />
	</adapters>



	<default-channels>
		<channel ref="my-streaming-amf" />
	</default-channels>

	<destination id="RecebimentoMessageServicePush">
